<?xml version="1.0"?>
<package >
  <metadata>
    <id>Lock-Stock-and-Auth</id>
    <version>0.2.0</version>
    <title>Lock Stock and Auth</title>
    <authors>Craig Pretorius</authors>
    <owners></owners>
    <description>
      This Nuget package is created to simplify the following authentication in Asp.Net Identity:

      - Cookie authentication for web sites
      - Token based authentication for HTML applications to Web Api
      - Token based authentication for HTML applications to OData
    </description>
    <releaseNotes>
    </releaseNotes>
    <summary>
      Nuget package for setting up authentication for a web application and a plain client using .NET Identity
    </summary>
    <language>en-US</language>
    <projectUrl>https://github.com/Halceyon/lock-stock-and-auth</projectUrl>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>https://github.com/Halceyon/lock-stock-and-auth/blob/master/LICENSE</licenseUrl>
    <copyright>Code HQ (Pty) Ltd Copyright 2017</copyright>
    <dependencies>
        <group targetFramework="net40">
          <dependency id="Autofac" version="4.0.0" />
          <dependency id="Autofac.Mvc5" version="4.0.0" />
          <dependency id="Autofac.WebApi2" version="4.0.1" />
          <dependency id="EntityFramework" version="6.2.0" />
          <dependency id="Microsoft.Owin.Cors" version="3.0.1" />
          <dependency id="Microsoft.AspNet.OData" version="6.0.0" />
          <dependency id="Microsoft.AspNet.Identity.Core" version="2.2.1" />
          <dependency id="Microsoft.AspNet.Identity.EntityFramework" version="2.2.1" />
          <dependency id="Microsoft.AspNet.Identity.Owin" version="2.2.1" />
          <dependency id="Microsoft.AspNet.WebApi.Client" version="5.2.4" />
          <dependency id="Microsoft.AspNet.WebApi.Core" version="5.2.4" />
          <dependency id="Microsoft.AspNet.WebApi.Owin" version="5.2.4" />
          <dependency id="Microsoft.Owin" version="4.0.0" />
          <dependency id="Microsoft.Owin.Host.SystemWeb" version="4.0.0" />
          <dependency id="Microsoft.Owin.Security" version="4.0.0" />
          <dependency id="Microsoft.Owin.Security.Cookies" version="4.0.0" />
          <dependency id="Microsoft.Owin.Security.Facebook" version="4.0.0" />
          <dependency id="Microsoft.Owin.Security.Google" version="4.0.0" />
          <dependency id="Microsoft.Owin.Security.OAuth" version="4.0.0" />
          <dependency id="Newtonsoft.Json" version="11.0.1" />
          <dependency id="Owin" version="1.0" />
        </group>
    </dependencies>
    <references></references>
    <tags></tags>
  </metadata>
